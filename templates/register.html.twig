{% extends 'base.html.twig' %}

{% block body %}
<h1>Register</h1>

{{ form_start(registrationForm) }}
{{ form_row(registrationForm.email) }}
{{ form_row(registrationForm.name) }}
{{ form_row(registrationForm.phoneNumber) }}
{{ form_row(registrationForm.password.first) }}
